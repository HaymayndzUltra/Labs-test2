{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Routing Log",
  "type": "object",
  "properties": {
    "session_id": {"type":"string"},
    "timestamp": {"type":"string","format":"date-time"},
    "decision": {"type":"string"},
    "confidence": {"type":"number"},
    "rules_considered": {"type":"array","items":{"type":"string"}},
    "winning_rule": {"type":"string"},
    "override_reason": {"type":["null","string"]},
    "approver": {"type":["null","string"]},
    "snapshot_id": {"type":["null","string"]}
  },
  "required":["session_id","timestamp","decision","rules_considered","winning_rule"]
}

